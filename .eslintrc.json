{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"],
    "indent": ["warn", 2],
    "no-console": "off"
  }
}
